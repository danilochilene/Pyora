            <discovery_rules>
                <discovery_rule>
                    <name>Discovery Oracle ASM volumes</name>
                    <key>pyora[{$USERNAME},{$PASSWORD},{$ADDRESS},{$PORT},{$DATABASE},show_asm_volumes]</key>
                    <delay>3600</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#ASMVOLUME}</macro>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>ASM Volume Use on {#ASMVOLUME}</name>
                            <key>pyora[{$USERNAME},{$PASSWORD},{$ADDRESS},{$PORT},{$DATABASE},asm_volume_use,{#ASMVOLUME}]</key>
                            <delay>300</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>ASM Volumes</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last(0)}&gt;{$HIGH}</expression>
                                    <name>Oracle/ASM Volume {#ASMVOLUME} used &gt; {$HIGH} %</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery Oracle tablespaces</name>
                    <key>pyora[{$USERNAME},{$PASSWORD},{$ADDRESS},{$PORT},{$DATABASE},show_tablespaces]</key>
                    <delay>3600</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#TABLESPACE}</macro>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Tablespace percentual use on {#TABLESPACE}</name>
                            <key>pyora[{$USERNAME},{$PASSWORD},{$ADDRESS},{$PORT},{$DATABASE},tablespace,{#TABLESPACE}]</key>
                            <delay>300</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>Tablespaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Tablespace free on {#TABLESPACE}</name>
                            <key>pyora[{$USERNAME},{$PASSWORD},{$ADDRESS},{$PORT},{$DATABASE},tablespace_abs,{#TABLESPACE}]</key>
                            <delay>300</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>Tablespaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{Pyora:pyora[{$USERNAME},{$PASSWORD},{$ADDRESS},{$PORT},{$DATABASE},tablespace,{#TABLESPACE}].last()}={$FS_P_USED_HIGH} and {Pyora:pyora[{$USERNAME},{$PASSWORD},{$ADDRESS},{$PORT},{$DATABASE},tablespace_abs,{#TABLESPACE}].last()}&lt;{$FS_FREE_LOW}</expression>
                            <name>Oracle/Tablespace {#TABLESPACE} used on {$DATABASE} high</name>
                            <priority>HIGH</priority>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Tablespace use on {#TABLESPACE}</name>
